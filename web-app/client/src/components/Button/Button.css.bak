@import url("../../colors.css");

.button-container {
  margin: 0rem 0.8rem;
}

.button-container button {
  position: relative;
  display: flex;
  justify-content: center;
  align-items: center;
  font-family: "Roboto", sans-serif;
  font-weight: 300;
  border-radius: 1000px;
  border: 0;
  color: var(--text-white);
}
